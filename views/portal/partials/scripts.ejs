<script type="text/javascript">
    $('#timeCheckBtn').change(function (e) {
        e.preventDefault();
        var inOutTrack = 'out'
        if (this.checked) inOutTrack = 'in'
        $.post("/timeTracker", {
            timeTrack: new Date().toLocaleTimeString(),
            inOutTrack: inOutTrack
        }).complete(function () {
            console.log("Success");
        });
    });
</script>
<script>
    $('#sidebarToggle').click(function(){
        if ($('#leftSpacer').hasClass('col-md-2')){
            $('#leftSpacer').removeClass('col-md-2')
            $('#left').removeClass('col-md-6').addClass('col-md-8')
        } 
        else {
            $('#leftSpacer').addClass('col-md-2')
            $('#left').removeClass('col-md-8').addClass('col-md-6')
        }
    })
</script>